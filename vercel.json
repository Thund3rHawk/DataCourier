{ 
    "version": 2, 
    "builds": [{
        "src": "src/index.ts",
        "use": "@vercel/node",
        "config": {
            "maxLambdaSize": "50mb",
            "memory": 1024,
            "timeout": 20
        }
    }],
    "routes": [{ 
        "src": "/(.*)", 
        "dest": "src/index.ts" 
    }],
    "functions":{
        "src/**/*": {
            "maxDuration": 20 
        }
    }
}